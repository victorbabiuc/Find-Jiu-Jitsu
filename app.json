{
  "expo": {
    "name": "JiuJitsu Finder",
    "slug": "OpenMatFinder",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "scheme": "jiujitsufinder",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.OpenMatFinder",
      "buildNumber": "19",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "JiuJitsu Finder uses your location to show nearby gyms on the map and calculate distances.",
        "NSPhotoLibraryUsageDescription": "JiuJitsu Finder needs access to your photo library to save images for sharing to Instagram Stories.",
        "NSPhotoLibraryAddUsageDescription": "JiuJitsu Finder needs access to save images to your photo library for sharing to Instagram Stories."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.OpenMatFinder"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "f0275a64-ee82-483a-aafa-6306353c10cd"
      }
    },
    "owner": "glootieapp"
  }
}
